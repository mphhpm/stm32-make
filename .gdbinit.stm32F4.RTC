	set $addr = $baseAddr + $TSDR
	set $name = "TSDR"
	printf "RTC.%-8s 0x%4x %4x: 0x%08x\n", $name, ($addr >> 16), ($addr & 0xFFFF), *$addr

	set $addr = $baseAddr + $TSSSR
	set $name = "TSSSR"
